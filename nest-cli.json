{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": ["@nestjs/swagger"],
    "assets": ["**/*.proto"],
    "watchAssets": true,
    "deleteOutDir": true
  },
  "generateOptions": {
    "spec": true,
    "flat": false
  },
  "defaultProject": "src",
  "monorepo": false,
  "projects": {},
  "entryFile": "main"
}
